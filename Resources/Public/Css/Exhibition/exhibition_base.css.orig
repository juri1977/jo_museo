/* Standalone - Frame um die Objekte und Sektionen */
.singleitem:after {
    content: '';
    height: 5px;
    position: absolute;
    background-color: #ffffff;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    -webkit-box-shadow: 0px 5px 15px 0px rgb(0 0 0 / 40%);
    -moz-box-shadow: 0px 5px 15px 0px rgb(0 0 0 / 40%);
    box-shadow: 0px 5px 15px 0px rgb(0 0 0 / 40%);
}
.single-logo {
    width: 3.5vw;
    margin-top: 5px;
    padding: 0.4vw 0;
    z-index: 1;
    background-color: rgba(0, 0, 0, 0.4);
}
.single-logo img {
    width: 2vw;
    display: block;
    height: auto;
    margin: 0.4vw auto;
}
.single-logo .main_title {
    writing-mode: vertical-rl;
    transform: rotate(180deg);
    display: inline-block;
    text-shadow: 2px 2px 2px rgba(8, 3, 2, 0.3);
    color: #ffffff;
    text-transform: uppercase;
    font-size: 1vw;
}

/* Zurück Button */
.exhibition .back {
    position: absolute;
    left: 3vw;
    top: 80px;
    z-index: 10;
    cursor: pointer;
}
.single-logo .back {
    top: unset;
    left: 0;
    bottom: -5px;
    transform: translateY(100%);
    background-color: rgba(0, 0, 0, 0.4);
}
.exhibition .back .joSprite {
    padding: 1.75vw;
    background-image: url(/typo3conf/ext/jo_museo/Resources/Public/Images/sprite_portal_white.svg);
    background-size: 75vw;
    background-position: -41.3vw -3.95vw;
    transform: rotateY(180deg);
    transition: opacity .5s ease;
    opacity: 0.8;
}
.exhibition .back .joSprite:hover {
    opacity: 1;
}

