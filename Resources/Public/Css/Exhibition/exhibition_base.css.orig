    border-top: 3px solid #dd7059;
    border-bottom: 3px solid #dd7059;
    padding: 30px 0;
    text-align: center;
}
.load_section {
    position: relative;
    transition: color .5s, border .5s;
}
.exhibition .content_wrap a.load_section:hover {
    border: 2px solid rgba(0,160,80,0);
    color: #dd7059;
}
.exhibition .content_wrap {
    padding-bottom: 659px;
}
.exhibition .joOpener {
    cursor: pointer;
}
.exhibition.showexhibition .item.teasertext {
    border-bottom: 0;
}
.subtype20 {
    padding: 60px 70px;
}
@media (min-width:99768px) {
    .joSection_container {
        height: 100vh;
    }
    .joSection_container .section_container .section .sectionInfo-c-content {
        opacity: 0;
    }
    .joSection_container .section_container .section.active .sectionInfo-c-content {
        opacity: 1;
    }
    .desc-con-audio .audio-intro {

        /*
        font-size: 24px;
        */
    }
    .joExhibition_content .section,
    .joObject_container .joObject {
        transition: all 1.2s;
        position: fixed;
        height: 130vh;
        transform: translateY(30vh);
        backface-visibility: hidden;
        transition: 1.2s all cubic-bezier(0.22, 0.44, 0, 1);
        overflow: hidden;
    }
    .joExhibition_content .section .content-wrapper,
    .joObject_container .joObject .content-wrapper {
        height: 100vh;
        color: #fff;
        font-family: Montserrat;
        transform: translateY(40vh);
        will-change: transform;
        backface-visibility: hidden;
        transition: 1.7s all cubic-bezier(0.22, 0.44, 0, 1);
    }
    .joExhibition_content .section.up-scroll,
    .joObject_container .joObject.up-scroll {
        transform: translate3d(0, -15vh, 0);
    }
    .joExhibition_content .section.up-scroll .content-wrapper,
    .joObject_container .joObject.up-scroll .content-wrapper {
        transform: translateY(15vh);
    }
    .joExhibition_content .section.up-scroll + .section,
    .joObject_container .joObject.up-scroll + .joObject {
        transform: translate3d(0, 30vh, 0);
    }
    .joExhibition_content .section.up-scroll + .section .content-wrapper,
    .joObject_container .joObject.up-scroll + .joObject .content-wrapper {
        transform: translateY(30vh);
    }
    .joExhibition_content .section.up-scroll .content-wrapper .sectionInfo-c-content,
    .joObject_container .joObject.up-scroll .content-wrapper .sectionInfo-c-content {
        transform: translateY(0);
    }
    .joExhibition_content .section.up-scroll + .section .content-wrapper .sectionInfo-c-content,
    .joObject_container .joObject.up-scroll + .joObject .content-wrapper .sectionInfo-c-content {
        transform: translateY(30vh);
    }
    .joExhibition_content .section.down-scroll,
    .joObject_container .joObject.down-scroll {
        transform: translate3d(0, -130vh, 0);
    }
    .joExhibition_content .section.down-scroll .content-wrapper,
    .joObject_container .joObject.down-scroll .content-wrapper {
        transform: translateY(30vh);
    }
    .joExhibition_content .section.down-scroll + .section:not(.down-scroll),
    .joObject_container .joObject.down-scroll + .joObject:not(.down-scroll) {
        transform: translate3d(0, -15vh, 0);
    }
    .joExhibition_content .section.down-scroll + .section:not(.down-scroll) .content-wrapper,
    .joObject_container .joObject.down-scroll + .joObject:not(.down-scroll) .content-wrapper {
        transform: translateY(15vh);
    }
    .joExhibition_content .section.down-scroll .content-wrapper .sectionInfo-c-content,
    .joObject_container .joObject.down-scroll .content-wrapper .sectionInfo-c-content {
        transform: translateY(15vh);
    }
    .joExhibition_content .section.down-scroll + .section:not(.down-scroll) .content-wrapper .sectionInfo-c-content,
    .joObject_container .joObject.down-scroll + .joObject:not(.down-scroll) .content-wrapper .sectionInfo-c-content {
        transform: translateY(0);
    }
}
