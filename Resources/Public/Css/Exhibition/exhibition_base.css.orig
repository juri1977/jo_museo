body {
    -ms-touch-action: pan-y;
    touch-action: pan-y;
}
.exhibition h1,
.exhibition h2,
.exhibition h3,
.exhibition h4,
.exhibition h5,
.exhibition h6 {
    font-family: 'Playfair Display', serif !important;
}
.exhibition .h1,
.exhibition h1 {
    font-size: 28px;
}
.exhibition .h2,
.exhibition h2 {
    position: relative;
    font-size: 20px;
}
.exhibition hr {
    margin-top: 18px;
    margin-bottom: 18px;
}
.exhibition h2 {
    font-size: 28px;
    font-weight: 300;
    padding-bottom: 0;
    margin-bottom: 30px;
}
.section_entry_image {
    background-repeat: no-repeat;
    background-size:cover;
    background-position: center;
}
.content-text a {

    /* text-decoration:underline; */
}
.section_img_title {
    font-weight: 400;
    padding: 5px 0;
}
.exhibition .min-height-100 {
    min-height: 100%;
}
.exhibition .flex-adjust {
    flex: 1;
}
.exhibition.showexhibition.overlay .teasertext::before,
.exhibition.showall .teasertext.overlay::before {
    content: '';
    display: block;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;    /* background-color: rgba(0,0,0,0.6); */
    position: absolute;
    z-index: 50;
    background-image: radial-gradient(circle 100vh at 50% 50%, rgba(0, 0, 0, 0.6), rgba(0, 0, 0, 0.1));
}
.exhibition.showexhibition h3 {
    font-size: 46px;
    font-family: 'Playfair Display', serif;
    padding: 20px 0;
    text-align: center;
    /*
    border-bottom: #000000 1px solid;
   
    margin-bottom: 60px;
     */
}
.exhibition.showexhibition h3.audio-title{
   font-size: 36px; 
}
.exhibition.showexhibition .text_layer h3 {
    margin-top: 20px;
    margin-bottom: 10px;
    padding: 0;
    border-bottom: #000000 0px solid;
    text-align: left;
}
.exhibition.showexhibition h4 {
    padding: 40px 0;
    font-size: 35px;
}
.exhibition.showexhibition .item,
.exhibition.showall .item {
    position: relative;
    border-bottom: #ffffff 1px solid;
    overflow: hidden;
    height: 100vh;
}
.exhibition.showexhibition .text_layer {

    /*
        position: absolute;
    bottom: 80px;
    left: 80px;
        */
    color: #ffffff;
    z-index: 80;
    padding: 0 15px 15px;
}
.exhibition.showall .text_layer {
    z-index: 60;
    padding: 0;
    position: absolute;
    bottom: 80px;
    color: #ffffff;
}
.exhibition.showall .text_layer h3 {
    margin-bottom: 0;
}
.exhibition.showall .item.left .text_layer {
    max-width: calc(100% - 20px);
    text-align: left;
}
.exhibition.showall .item.view_6col .text_layer {
    max-width: calc(100% - 5vw);
    left: 5vw;
}
.exhibition.showall .item.view_default .text_layer {
    left: 50%;
    transform: translateX(-50%);
    text-align: center;
}
.exhibition.showexhibition .item.teasertext .text_layer {
    margin-top: 150px;
    flex: 1;
}
.exhibition.showall .item.right .text_layer {
    right: 80px;
    text-align: right;
}
.exhibition.showexhibition img.img,
.exhibition.showall img.img {
    width: 100%;
    height: auto;
}
.exhibition.showexhibition .text_layer h3,
.exhibition.showall .text_layer h3 {
    font-size: 46px;
    line-height: 120%;
    font-family: 'Playfair Display', serif;
}
.exhibition.showexhibition .text_layer h4 {
    text-align: left;
}
.exhibition.showexhibition .text_layer h4,
.exhibition.showall .text_layer h4 {
    font-size: 26px;
    padding: 20px 0;
    font-family: 'Roboto Condensed', sans-serif !important;
}
.exhibition.showexhibition .more_link {
    color: #ffffff;
    border: 2px solid currentColor;
    padding: 12px 24px;
    background: none;
    display: inline-block;
    margin: 20px 0 0;
    text-transform: uppercase;
    line-height: 100%;
    text-decoration: none;
}

/* .exhibition.showexhibition .more_link {
margin:0 auto;
} */
.joExhibition_content img {
    width: 100%;
    height: auto;
}
.joExhibition_content .section {
    position: relative;
    height: 100vh;
    overflow: hidden;
    background: rgb(65, 65, 65);
    background: linear-gradient(180deg, rgba(65, 65, 65, 1) 0%, rgba(29, 29, 29, 1) 100%);
}
.map-scroll {
    width: 100%;
}
.joExhibition_content .section_container .section:first-child {
    margin-top: 0;
}
.text_layer_more {
    z-index: 80;
    margin-top: auto;
    margin-bottom: 20px;
}
.load_section {
    display: block;
    padding: 15px;
    border: 2px solid white;
    margin: 0 auto 30px;
    color: white;
    font-weight: 600;
    transition: background-color .5s ease, border-color .5s ease, color .5s ease;
}
.content_wrap a.load_section {
    color: white;
}

/*
.content_wrap a.load_section:hover {
    border-color: #dd7059;
    color: #dd7059;
}
*/
.section .img-description {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 30%;
    color: white;
    padding: 10px;
    font-size: 12px;
    z-index: 100;
    text-align: left;
}
.joExhibition_content .sectionInfo-container {
    font-size: 16px;
    position: absolute;
    top: 0;
    width: 100%;
    height: 100%;
    color: white;
    z-index: 10;
}
.joExhibition_content .sectionInfo-container .sectionInfo-c {

    /* background-color: rgba(0,0,0,.2); */
    height: 100%;
}
.sectionInfo-c-btn-container {
    position: relative;
}
.sectionInfo-c-btn-container::after,
.sectionInfo-c-btn-container::before {
    content: "";
    width: calc(50% - 50px);
    height: 3px;
    right: 0;
    top: 50%;
    position: absolute;
    background-color: rgba(221, 112, 89, 1);
}
.sectionInfo-c-btn-container::before {
    left: 0;
    right: unset;
}
.joExhibition_content .sectionInfo-container .sectionInfo-c-content {
    padding: 15px;
    text-align: center;    /* background-color: rgba(0,0,0,0.6); */
    height: 100%;
}
.joExhibition_content .sectionInfo-container .sectionText {
    padding: 20px 0;
}
.joExhibition_content .sectionInfo-c-btn {
    z-index: 30;
    width: 70px;
    height: 70px;
    margin: 0 auto;
    padding: 5px;
    border-radius: 100%;
    background-color: rgba(221, 112, 89, .5);
    cursor: pointer;    /* transform: translateX(200px) rotate(135deg) scale(1); */
    transform: scale(1);
    transition: transform cubic-bezier(0.5, 1.8, 0.8, 0.5) 2s, right 1s ease-in-out, width 2s ease-in-out, height 2s ease-in-out;
}

/* .joExhibition_content .sectionInfo-c-btn.active {
right: 0;
z-index: 50;
transform: translateX(0px) rotate(0deg) scale(1);
} */
.joExhibition_content .sectionInfo-c-btn.active {
    animation: bigger 2s ease;
}
@keyframes bigger {
    0% {
        transform: scale(1);
    }
    50% {
        transform: scale(1.2);
    }
    100% {
        transform: scale(1);
    }
}
.joExhibition_content .sectionInfo-c-btn.active:hover {
    transform: scale(1.2);
}
.joExhibition_content .sectionInfo-c-btn-icon {
    background-image: url(/typo3conf/ext/jo_museo/Resources/Public/Images/sprite_portal_white.svg);
    color: transparent;
    text-align: center;
    cursor: pointer;
    display: block;
    background-color: transparent;
    background-size: 1400px;
    background-repeat: no-repeat;
    background-position: -1266px -7px;
    width: 60px;
    height: 60px;
    transform: translate(-50%, -50%);
    top: 50%;
    position: absolute;
    left: 50%;
}
.intro_media_con {
    background-color: #d6d6d6;
    padding-top: 30px;
    padding-bottom: 30px;
}

/* .joExhibition_content .sectionInfo-c-btn.collapsed {
    transform: translate(50%, -50%) rotate(180deg);
}*/
.sectionInfo-line {
    width: 100%;
    height: 2px;
    background-color: rgba(221, 112, 89, 1);
    position: absolute;
    top: calc(50% - 1px);
    left: 0;
    display: none;
}
.sectionTitle-container {
    padding: 5% 0 5% 0;    /* text-transform: uppercase; */
}
.sectionTitle-container h3 {
    font-size: 24px;
    font-size: 2.5vw;
    color: #dd7059;
    word-wrap: break-word;
    -webkit-hyphens: auto;
    -ms-hyphens: auto;
    hyphens: auto;
}
.exhibition.showall .text_layer h3,
.exhibition.showall .text_layer h4,
.exhibition.showall .text_layer .item-period,
.exhibition.showexhibition .text_layer h3,
.exhibition.showexhibition .text_layer h4,
.exhibition .with_image .sektion-title,
.sectionText,
.sectionTitle-container h3,
.sectionSubTitle {
    text-shadow: 0px 0px 12px rgba(0, 0, 0, 0.8), 0px 0px 8px rgba(0, 0, 0, 0.3), 0px 0px 4px rgba(0, 0, 0, 0.1);
}
.sectionSubTitle {
    font-size: 1.8vw;
}
.exhibition .with_image .sektion-title {
    text-align: center;
}
.joExhibition_content .more-arrow,
.joObject .more-arrow {
    width: 60px;
    height: 60px;
    background-color: green;
    position: absolute;
    bottom: 50px;
    left: 50%;
    transform: translateX(-50%) rotate(90deg);
    background-image: url(/typo3conf/ext/jo_museo/Resources/Public/Images/sprite_portal_white.svg);
    color: transparent;
    text-align: center;
    cursor: pointer;
    display: block;
    background-color: transparent;
    background-size: 2000px;
    background-repeat: no-repeat;
    background-position: -921px -423px;
    opacity: 0;
    visibility: hidden;
}
.joExhibition_content .section.active .more-arrow,
.joObject.active .more-arrow {
    animation: breathopacity 4s ease;
}
@keyframes breathopacity {
    0% {
        opacity: 0;
        visibility: visible;
    }
    25% {
        opacity: 1;
        visibility: visible;
    }
    50% {
        opacity: 0;
        visibility: visible;
    }
    75% {
        opacity: 1;
        visibility: visible;
    }
    100% {
        opacity: 0;
        visibility: hidden;
    }
}
.joText-btn-icon {
    width: 40px;
    height: 40px;
    background-image: url(/typo3conf/ext/jo_museo/Resources/Public/Images/sprite_portal_black.svg);
    color: transparent;
    text-align: center;
    cursor: pointer;
    display: block;
    background-color: transparent;
    background-size: 900px;
    background-repeat: no-repeat;
    background-position: -767px -49px;
    position: absolute;
    right: 31px;
    bottom: -40px;
    cursor: pointer;
    display: none;
}
.joText-btn-icon.hide {
    background-position: -1043px -69px;
    background-size: 1150px;
}
.joText-btn-icon.active {
    display: block;
}
